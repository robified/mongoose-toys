<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Document</title>
        <link rel="stylesheet" href="/stylesheets/style.css" />
    </head>
    <body>
        <% toys.forEach(function(toy) { %> 
            <p><%= toy.name%></p>
            <p><%= toy.yearMade%></p>
            <p><%= toy.maker %></p>
            <p><%= toy.stillAvailable %></p><hr>
        <% }); %>
    </body>
</html>
